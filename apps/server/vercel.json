{
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "",
    "devCommand": "uvicorn app.main:app --reload --port $PORT"
}